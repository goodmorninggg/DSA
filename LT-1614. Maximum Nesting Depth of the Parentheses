class Solution {
    public int maxDepth(String s) {
        int curr=0;
        int max=0;
        for(char c:s.toCharArray()){
            if(c=='('){
                curr+=1;
                if(curr>max){
                    max=curr;
                }
            }else if(c==')'){
                curr--;
            }
        }
        return max;
    }
}
